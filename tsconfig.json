{
  "compilerOptions": {

    "outDir": "./bin",                                  
    "declaration": true,                                  
    "target": "es2016",                                  
    "moduleDetection": "force",                       
    /* Modules */
    "module": "es2015",                              
    "moduleResolution": "node16",                              
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    /* Type Checking */
    "strict": false,                                     
    "skipLibCheck": true,
    "noImplicitAny": false
  }
}
